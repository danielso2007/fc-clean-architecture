{
  "name": "fc-ddd-patterns",
  "version": "1.0.0",
  "private": true,
  "description": "Projeto de estudo Clean Architecture",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=20"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "dev": "nodemon src/infrastructure/api/server.ts",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,json,md}\"",
    "test:watch": "jest --watch",
    "test": "npm run tsc -- --noEmit && jest",
    "tsc": "tsc",
    "release": "standard-version",
    "changelog": "standard-version --dry-run",
    "prepare": "husky",
    "commit": "npx git-cz",
    "lint:commit": "commitlint --from=HEAD~1",
    "lint": "eslint 'src/**/*.ts' --ext .ts --no-ignore",
    "lint:fix": "eslint 'src/**/*.ts' --ext .ts --no-ignore --fix"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielso2007/fc-clean-architecture.git"
  },
  "keywords": [
    "ddd",
    "architecture",
    "typescript"
  ],
  "author": "Daniel Oliveira",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.38.0",
    "@swc/core": "^1.15.3",
    "@swc/jest": "^0.2.39",
    "@types/jest": "^29.5.14",
    "@types/jstoxml": "^5.0.0",
    "@types/node": "^24.7.2",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "commitizen": "^3.0.0",
    "commitlint": "^20.1.0",
    "cz-conventional-changelog": "^3.0.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.0",
    "standard-version": "^9.5.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@types/express": "^5.0.3",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jstoxml": "3.2.3",
    "nodemon": "^3.1.10",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "sqlite3": "^5.1.7",
    "uuid": "8.3.2",
    "yup": "^1.7.1"
  }
}
